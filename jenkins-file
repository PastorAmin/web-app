node {  
    stage('def mvn-home= tool name: '', type: 'maven')
    stage( 'Git-Checkout'){ 
        git branch: 'main', credentialsId: 'git-cred', url: 'https://github.com/PastorAmin/web-app.git'
    }
    stage('clean-compile') { 
        $(mvn-home)/bin/mvn clean compile
    }
    stage('package') { 
        $(mvn-home)/bin/mvn package
    }
}
